[run]
source = kin_base,tests

[report]
omit =
    kin_base/stellarxdr/*
    kin_base/base58.py
    kin_base/version.py
    kin_base/purecrc16.py

exclude_lines =
    pragma: no cover

    def __repr__
    if self\.debug

    raise AssertionError
    raise NotImplementedError

    if 0:
    if __name__ == .__main__.: